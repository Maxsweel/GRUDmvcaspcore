@model AplicacaoMysql.Models.Contato

@{ ViewData["Title"] = "Detalhes do Contato";}

<h1>Detalhes</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Nome)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Nome)</dd>

        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Email)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Email)</dd>

        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Tel)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Tel)</dd>

        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Msg)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Msg)</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Atualizar</a> |
    <a asp-action="Index">Kista de Contatos</a>
</div>
